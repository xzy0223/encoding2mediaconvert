        <color_range>1</color_range>
        <transfer_characteristics>1</transfer_characteristics>
        <color_primaries>1</color_primaries>
        <matrix_coefficients>1</matrix_coefficients>
        <level>31</level>
    </video_codec_parameters>
    <video_sync>passthrough</video_sync>
    <encoder>edc1</encoder>
    <destination>http://xxx</destination>
