<?xml version="1.0"?>
<query>
    <userid>mock_mock_test_user_id</userid>
    <userkey>mock_mock_test_key</userkey>
    <action>AddMedia</action>
    <source>mock_mock_source_rendition__url</source>
    <notify>mock_mock_encoding_notify_url</notify>
    <notify_format>json</notify_format>
    <notify_body_format>raw</notify_body_format>
    <format>   <!-- C4_YouView_HD_MP4_1920x1080_Test-->
        <closed_captions>
            <mux_type>dvb</mux_type>
            <font_source>https://dvbsubtitlefonts.channel4.com/TiresiasScreenfont.ttf</font_source>
        </closed_captions>   
        <output>mpegts</output>   
        <size>1920x1080</size>   
        <video_codec>libx264</video_codec>   
        <cbr>yes</cbr>    
        <hard_cbr>yes</hard_cbr>    
        <nal-hrd>cbr</nal-hrd>   
        <bitrate>5000k</bitrate>   
        <framerate>25</framerate>   
        <keyframe>50</keyframe>   
        <keyint_min>50</keyint_min>   
        <two_pass>yes</two_pass>   
        <audio_codec>dolby_heaacv2</audio_codec>   
        <audio_sample_rate>48000</audio_sample_rate>   
        <audio_bitrate>160k</audio_bitrate>   
        <audio_channels_number>2</audio_channels_number>
        <destination>http://test_url.s3.amazonaws.com/encoding.com.output/mock_mock_output_rendition_name_11111_1039.ts?acl=public-read</destination>
    </format>
</query>