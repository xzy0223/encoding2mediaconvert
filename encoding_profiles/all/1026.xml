<?xml version="1.0"?>
<query>
    <userid>mock_mock_test_user_id</userid>
    <userkey>mock_mock_test_key</userkey>
    <action>AddMedia</action>
    <source>mock_mock_source_rendition__url</source>
    <notify>mock_mock_encoding_notify_url</notify>
    <notify_format>json</notify_format>
    <notify_body_format>raw</notify_body_format>
    <format>
        <closed_captions>
            <cea_stream>auto</cea_stream>
            <cea_data_field>0</cea_data_field>
            <mux_type>webvtt</mux_type>
            <ignore_cc_absence>yes</ignore_cc_absence>
            <direct_copy>no</direct_copy>
            <font_size>16</font_size>
            <copy>yes</copy>
            <role>subtitle</role>
            <add_source_offset>no</add_source_offset>
            <forced>no</forced>
            <get_from_manifest>no</get_from_manifest>
            <transparent_background>no</transparent_background>
            <cc_extract_before>segmentation</cc_extract_before>
        </closed_captions>
        <output>advanced_dash</output>
        <turbo>no</turbo>
        <twin_turbo>no</twin_turbo>
        <segment_duration>6</segment_duration>
        <pack_files>yes</pack_files>
        <pack_type>tar</pack_type>
        <pack_each_stream>no</pack_each_stream>
        <segmenter>v3</segmenter>
        <copy_nielsen_metadata>no</copy_nielsen_metadata>
        <file_extension>tar</file_extension>
        <encoder>edc1</encoder>
        <duration_precision>5</duration_precision>        
        <add_audio_only>no</add_audio_only>
        <merge_small_segments>no</merge_small_segments>
        <AdditionalCfDestination></AdditionalCfDestination>
        <metadata>
            <copy>no</copy>
        </metadata>
        <dash_profile>live</dash_profile><!--Video Stream 1024x576 @ 1400k -->  
        <stream>    
            <video_only>yes</video_only>    
            <video_codec>libx264</video_codec>    
            <profile>baseline</profile>    
            <size>1024x576</size>    
            <bitrate>1400k</bitrate>    
            <maxrate>1500k</maxrate>    
            <bufsize>1200k</bufsize>    
            <cbr>no</cbr>    
            <keyframe>50</keyframe>    
            <framerate>25</framerate>    
            <pix_format>yuv420p</pix_format>    
            <set_aspect_ratio>16:9</set_aspect_ratio>    
            <keep_aspect_ratio>yes</keep_aspect_ratio>    
            <pixel_aspect_ratio>1:1</pixel_aspect_ratio>    
            <two_pass>yes</two_pass>    
            <deinterlacing>auto</deinterlacing>    
            <video_codec_parameters>      
                <keyint_min>5</keyint_min>      
                <level>30</level>      
                <sc_threshold>0</sc_threshold>      
                <bf>0</bf>      
                <b_strategy>0</b_strategy>      
                <nal-hrd>vbr</nal-hrd>    
            </video_codec_parameters>    
        </stream><!--Audio Stream 48kHz @ 128k -->
        <stream>
            <audio_only>yes</audio_only>
            <audio_codec>libfaac</audio_codec>
            <audio_bitrate>128k</audio_bitrate>
            <audio_sample_rate>48000</audio_sample_rate>
            <audio_channels_number>2</audio_channels_number>
            <audio_volume>70</audio_volume>
        </stream>
        <destination>http://test_url.s3.amazonaws.com/encoding.com.output/mock_mock_output_rendition_name_11111_1026.tar?acl=public-read</destination>
    </format>
</query>